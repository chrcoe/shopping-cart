<?xml version="1.0" encoding="UTF-8"?>
<TransactionPolicy>
	<Transaction id="business.CheckOut">
		<Policy id="business.policy.IsUserRegistered" />
		<Policy id="business.policy.HasSufficientInventory" />
		<Policy id="business.policy.UpdateProfileStatus">
			<Parameter id="MinOrders" value="5" />
		</Policy>
		<Policy id="business.policy.Shipping">
			<Parameter id="Rate" value=".015" />
			<Parameter id="PremiumRate" value="0.0" />
			<Parameter id="PremiumMinOrders" value="5" />
		</Policy>
	</Transaction>
	<Transaction id="business.UpdateCart">
		<Policy id="business.policy.Shipping" >
			<Parameter id="Rate" value=".015" />
			<Parameter id="PremiumRate" value="0.0" />
		</Policy>
	</Transaction>
</TransactionPolicy>