<?xml version="1.0" encoding="UTF-8"?>
<TransactionPolicy>
	<Transaction id="business.CheckOut">
		<Policy id="business.policy.IsRegisteredPolicy" rule="Pre" />
		<Policy id="business.policy.IsProductInStock" rule="Pre" />
		<Policy id="business.policy.IsProfilePremiumStatus" rule="Post">
			<Parameter id="MinOrders" value="5" />
		</Policy>
		<Policy id="business.policy.Shipping" rule="Pre">
			<Parameter id="rate" value=".02" />
			<Parameter id="PremiumRate" value="0.0" />
		</Policy>
	</Transaction>
	<Transaction id="business.AddItemToCart">
		<Policy id="business.policy.HasItemReservation" rule="Pre" />
	</Transaction>
</TransactionPolicy>